    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "express": "^4.14.0",
    "node-sass": "^3.8.0",
    "nodemon": "^1.10.2",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "express": "4.14.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
